<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/application_7.xsd" version="7">
  <description>A sample Java EE application that tries to use as many Java EE techology standards as possible.</description>
  <display-name>jee-ear</display-name>
  <module>
    <web>
      <web-uri>jee-web.war</web-uri>
      <context-root>/jee</context-root>
    </web>
  </module>
  <module>
    <ejb>jee-domain.jar</ejb>
  </module>
  <security-role>
    <role-name>users</role-name>
  </security-role>
  <library-directory>lib</library-directory>
</application>
