#!/usr/bin/expect

set timeout 600
spawn -noecho collective join defaultServer --host=controller --port=9443 --keystorePassword=memberPassword --user=adminUser --password=adminPassword

expect {
  (y/n) {
    send -- "y\n"
    exp_continue
  }
  timeout {
    exit 1
  }
  eof {
    catch wait result
    exit [lindex $result 3]
  }
}